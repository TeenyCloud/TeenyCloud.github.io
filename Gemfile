source "https://rubygems.org"

# GitHub Pages gem - maintains compatibility with GitHub Pages environment
# To upgrade, run `bundle update github-pages`
gem "github-pages", group: :jekyll_plugins

# Jekyll plugins
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
end

# Required for Ruby 3.0+
gem "webrick", "~> 1.8"
